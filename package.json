{
  "name": "lang3",
  "displayName": "Lang3",
  "description": "Language support for Lang3",
  "version": "0.0.1",
  "publisher": "FinnE145",
  "author": {
    "name": "Finn",
    "email": "finne014@gmail.com",
    "url": "https://github.com/FinnE145"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FinnE145/Lang3Extension"
  },
  "license": "GPL-3.0-only",
  "engines": {
    "vscode": "^1.88.0"
  },
  "icon": "images/Lang3LogoSquare.png",
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "lang3",
        "aliases": ["Lang 3", "lang3"],
        "extensions": [".lang3", ".l3"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lang3",
        "scopeName": "source.lang3",
        "path": "./syntaxes/lang3.tmLanguage.json"
      },
      {
        "scopeName": "markdown.lang3.codeblock",
        "path": "./syntaxes/lang3-markdown.tmLanguage.json",
        "injectTo": ["text.html.markdown"],
        "embeddedLanguages": {
          "meta.embedded.block.lang3": "lang3"
        }
      }
    ],
    "themes": [
      {
        "label": "Lang3 Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/lang3-dark-theme.json"
      }
    ]
  }
}
